{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    {
      "src": ".*",
      "headers": {
        "strict-transport-security": "max-age=63072000; includeSubDomains; preload"
      },
      "continue": true
    },
    {
      "src": "^/(js|css|img)/.*",
      "headers": { "cache-control": "max-age=31536000, immutable" }
    },
    { "handle": "filesystem" },
    {
      "src": "/.*",
      "status": 301,
      "headers": { "location": "/" }
    }
  ],
  "alias": "kidonng.me"
}
